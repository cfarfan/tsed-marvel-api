{
  "name": "tsed-marvel-api",
  "version": "1.0.0",
  "description": "Marvel Heroes API using ts.ED",
  "main": "index.js",
  "author": "Camilo Farf√°n <camilo.farfanp@gmail.com>",
  "license": "ISC",
  "scripts": {
    "build": "npm run tsc",
    "coverage": "npm run test-cov && npm run test-remap",
    "postinstall": "npm run build",
    "tslint": "tslint ./*.ts ./lib/*.ts",
    "test": "mocha --reporter spec --check-leaks --bail test/",
    "tsc": "tsc --project tsconfig.json",
    "tsc:w": "tsc -w",
    "start": "nodemon --watch '**/*.ts' --ignore 'node_modules/**/*' --exec ts-node app.ts"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "nodemon": "^1.17.5",
    "ts-node": "^6.1.1",
    "tslint-config-prettier": "^1.13.0",
    "typescript": "^2.9.2"
  },
  "dependencies": {
    "@tsed/common": "^4.23.2",
    "@tsed/core": "^4.23.2",
    "body-parser": "^1.18.3",
    "compression": "^1.7.2",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.3",
    "method-override": "^2.3.10",
    "morgan": "^1.9.0"
  }
}
